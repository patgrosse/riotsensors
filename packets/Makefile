include Makefile.inc

all: $(RS_PACKETS_LIB_FILE)

objs: $(RS_PACKETS_OBJS)

clean:
	$(RM) $(RS_PACKETS_LIB_FILE)
	$(RM) $(LIBSPT_OBJS)

$(RS_PACKETS_LIB_FILE): $(RS_PACKETS_OBJS)
	$(AR) rcs $(RS_PACKETS_LIB_FILE) $(LIBSPT_OBJS)

$(RS_PACKETS_OBJS_DIR)/%.o: %.c
	$(CC) $(RS_PACKETS_CFLAGS) $(RS_PACKETS_INCLUDES) -c $< -o $@

$(RS_PACKETS_OBJS): | $(RS_PACKETS_OBJS_DIR)

$(RS_PACKETS_OBJS_DIR):
	$(MKDIR) -p "$@"
