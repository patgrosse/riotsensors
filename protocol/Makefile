include Makefile.inc

all: $(RS_PROTOCOL_LIB_FILE)

objs: $(RS_PROTOCOL_OBJS)

clean:
	$(RM) $(RS_PROTOCOL_LIB_FILE)
	$(RM) $(LIBSPT_OBJS)

$(RS_PROTOCOL_LIB_FILE): $(RS_PROTOCOL_OBJS)
	$(AR) rcs $(RS_PROTOCOL_LIB_FILE) $(RS_PROTOCOL_OBJS)

$(RS_PROTOCOL_OBJS_DIR)/%.o: %.c
	$(CC) $(RS_PROTOCOL_CFLAGS) $(RS_PROTOCOL_INCLUDES) -c $< -o $@

$(RS_PROTOCOL_OBJS): | $(RS_PROTOCOL_OBJS_DIR)

$(RS_PROTOCOL_OBJS_DIR):
	$(MKDIR) -p "$@"
